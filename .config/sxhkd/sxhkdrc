# Reload sxkhd
control + shift + r
    pkill -USR1 -x sxhkd

# Monitor select
super + F3  
    monitor_select

# Browser Select
super + F1
    browser_select

# Volume Controls
super + {button4,button5,Alt_L} 
    volume_controls {up,down,mute}

{XF86AudioRaiseVolume,XF86AudioLowerVolume,XF86AudioMute}
    volume_controls {up,down,mute}

# System power actions select
super + F7
    power_actions

# xclip testing
control + F1
    xclip -o >> /tmp/clipstore

# xclip testing
control + F2
    cat /tmp/clipstore | xclip -sel primary

# Test Control
super + F12
    notify-send "$GNUPGHOME"
